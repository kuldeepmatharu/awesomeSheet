<section id="display-notes" class="l-display js-display is-hidden">
  <button class="m-display-self-link button button-link js-display-self-link" data-self-link="#display-notes"></button>
  <button class="m-display-edit-link button button-link button-small button-icon u-inline-with-input js-display-edit-link" data-edit-link="#notes"><span class="icon-edit"></span></button>
  <div class="m-display-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <h1 class="m-display-title">Notes</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12">

          <div class="m-display-body">
            <p class="m-display-body-title">Character</p>
            <ul class="m-display-grid-1-col u-list-unstyled js-display-block" data-display-type="clone" data-display="notes.character">
            </ul>
          </div>

          <div class="m-display-body">
            <p class="m-display-body-title">Story</p>
            <ul class="m-display-grid-1-col u-list-unstyled js-display-block" data-display-type="clone" data-display="notes.story">
            </ul>
          </div>

        </div>
      </div>
    </div>
  </div>
</section>
