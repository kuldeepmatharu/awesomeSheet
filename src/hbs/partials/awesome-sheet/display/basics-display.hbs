<div class="container">
  <div class="row">
    <div class="col-xs-12">

      <div class="m-placeholder js-placeholder-display">
        <p class="m-placeholder-description u-muted-text"><strong>No Basics information</strong> added yet, use <strong class="m-placeholder-key"><span class="icon-edit"></span> Edit</strong> to add some.</p>
      </div>

    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">

      <div class="m-display-name m-display-block js-display-block is-hidden" data-display-content="false">
        <div class="m-display-box">
          <div class="m-display-box-body">
            <h1 class="u-no-margin js-display-block-target" data-display-type="text-snippet" data-display-path="basics.name"></h1>
            <h2 class="m-display-box-accent js-display-block-target" data-display-type="text-snippet" data-display-path="basics.initiative.current,basics.speed,skills.perception.current" data-display-prefix="Initiative,Speed,Perception" data-display-value-type="bonus,,bonus"></h2>
            <p class="m-display-box-text js-display-block-target" data-display-type="text-snippet" data-display-path="basics.race,basics.class,basics.level,basics.xp,basics.alignment,basics.size.category,basics.height,basics.weight,basics.age,basics.luck_points,basics.hero_points" data-display-prefix=",,Level,,,,,,,Luck points,Hero points" data-display-suffix=",,,XP,,,tall,,years old,,,"></p>
          </div>
        </div>
      </div>

    </div>
  </div>
</div>
