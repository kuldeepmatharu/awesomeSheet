<section id="display-offense" class="l-display js-display is-hidden">
  <button class="m-display-self-link button button-link js-display-self-link" data-self-link="#display-offense"></button>
  <button class="m-display-edit-link button button-link button-small button-icon u-inline-with-input js-display-edit-link" data-edit-link="#offense"><span class="icon-edit"></span></button>
  <div class="m-display-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <h1 class="m-display-title">Offense</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12">

          <div class="m-display-body">
            <p class="js-display-block" data-display-type="text-snippet" data-display="offense.base_attack,offense.cmb.current,offense.cmd.current,offense.melee_attack.current,offense.ranged_attack.current" data-display-prefix="BAB ,CMB ,CMD ,Melee ,Ranged ">
              <span class="js-display-block-target"></span>
            </p>
          </div>

          <div class="m-display-body">
            <p class="m-display-body-title">Melee Attacks</p>
            <ul class="m-display-list u-list-unstyled js-display-block" data-display-type="clone" data-display="offense.attack.melee">
            </ul>
          </div>

          <div class="m-display-body">
            <p class="m-display-body-title">Ranged Attacks</p>
            <ul class="m-display-list u-list-unstyled js-display-block" data-display-type="clone" data-display="offense.attack.ranged">
            </ul>
          </div>

          <div class="m-display-body">
            <p class="js-display-block" data-display-type="text-snippet" data-display="offense.attack_notes" data-display-title="Notes ">
              <span class="js-display-block-target"></span>
            </p>
          </div>

        </div>
      </div>
    </div>
  </div>
</section>
