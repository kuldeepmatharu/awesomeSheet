<section id="display-offense" class="l-display js-display is-hidden" data-display-title="Offense" data-display-icon="icon-combat" data-edit-jump="offense">
  <button data-self-link="display-offense" class="m-display-self-link button button-link js-display-self-link"></button>
  <div class="m-display-section-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">

          <div class="m-display-body">
            <p class="js-display-block" data-display-type="text-snippet" data-display="offense.base_attack,offense.cmb.current,offense.cmd.current,offense.melee_attack.current,offense.ranged_attack.current" data-display-prefix="BAB ,CMB ,CMD ,Melee ,Ranged ">
              <span class="js-display-block-target"></span>
            </p>
          </div>

          <div class="m-display-body">
            <p class="m-display-body-title">Melee</p>
            <ul class="m-display-grid-1-col u-list-unstyled js-display-block" data-display-type="clone" data-display="offense.attack.melee">
            </ul>
          </div>

          <div class="m-display-body">
            <p class="m-display-body-title">Ranged</p>
            <ul class="m-display-grid-1-col u-list-unstyled js-display-block" data-display-type="clone" data-display="offense.attack.ranged">
            </ul>
          </div>

          <div class="m-display-body">
            <p class="js-display-block" data-display-type="text-snippet" data-display="offense.attack_notes" data-display-title="Notes ">
              <span class="js-display-block-target"></span>
            </p>
          </div>

        </div>
      </div>
    </div>
  </div>
</section>
