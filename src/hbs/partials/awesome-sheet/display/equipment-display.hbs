<div class="container">
  <div class="row">
    <div class="col-xs-12">

      <div class="m-display-body">
        <p class="m-display-body-title">Gear</p>
        <p class="js-display-block" data-display-type="text-block" data-display="equipment.gear">
          <span class="js-display-block-target"></span>
        </p>
        <p class="m-display-body-title">Magic Gear</p>
        <p class="js-display-block" data-display-type="text-block" data-display="equipment.magic_gear">
          <span class="js-display-block-target"></span>
        </p>
      </div>

      <div class="m-display-body">
        <p class="m-display-body-title">Body Slots</p>
        <ul class="m-display-grid u-list-unstyled js-display-block" data-display-type="list" data-display="equipment.body_slots.armor,equipment.body_slots.belts,equipment.body_slots.body,equipment.body_slots.chest,equipment.body_slots.eyes,equipment.body_slots.feet,equipment.body_slots.hands,equipment.body_slots.head,equipment.body_slots.headband,equipment.body_slots.neck,equipment.body_slots.ring_left_hand,equipment.body_slots.ring_right_hand,equipment.body_slots.shield,equipment.body_slots.shoulders,equipment.body_slots.wrist" data-display-prefix="Armor ,Belts ,Body ,Chest ,Eyes ,Feet ,Hands ,Head ,Headband ,Neck ,Ring (Left Hand) ,Ring (Right Hand) ,Shield ,Shoulders ,Wrist ">
