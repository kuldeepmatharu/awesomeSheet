      <div class="m-display-body">
        <p class="m-display-body-title">Wealth</p>
        <p class="js-display-block" data-display-type="text-snippet" data-display="equipment.wealth.platinum,equipment.wealth.gold,equipment.wealth.silver,equipment.wealth.copper" data-display-suffix=" pp, gp, sp, cp">
          <span class="js-display-block-target"></span>
        </p>
      </div>

      <div class="m-display-body">
        <p class="m-display-body-title">Consumables</p>
        <ul class="m-display-grid u-list-unstyled js-display-block" data-display-type="clone" data-display="equipment.consumable">
        </ul>
      </div>

    </div>
  </div>
